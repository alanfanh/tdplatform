{% extends "base.html" %}
{% block title %}blog titles{% endblock %}

{% block content %}
<div class="col-md-2">
{% include "asset/leftslider.html" %}
</div>
<div class="row text-center vertical-middle-sm">
    {% comment %} <h1>文章</h1> {% endcomment %}
</div>
<div class="row">
    <div class="col-xs-12 col-md-8">
    <ul>
    {% for blog in blogs %}
        <li><a href="{% url 'asset:blog_detail' blog.id %}">{{ blog.title }}</a></li>
    {% endfor %}
    </ul>
    </div>
</div>
{% endblock %}